<odoo>
    <record id="sale_order_view_form" model="ir.ui.view">
        <field name="name">sale_order_view_form</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='order_line']//tree//field[@name='product_uom_qty']" position="before">
                <field name="commission_id" column_invisible="1"/>
                <field name="is_invoiced" column_invisible="1"/>
                <field name="commission_percent" readonly="is_invoiced == True"/>
                <field name="in_commission_percent" readonly="is_invoiced == True"/>
                <field name="out_commission_percent" readonly="is_invoiced == True"/>

                <field name="in_commission_amount" column_invisible="1"/>
                <field name="out_commission_amount" column_invisible="1"/>
                <field name="commission_amount" column_invisible="1"/>
            </xpath>
            <xpath expr="//field[@name='user_id']" position="attributes">
                <attribute name="domain">[]</attribute>
            </xpath>
        </field>
    </record>

</odoo>