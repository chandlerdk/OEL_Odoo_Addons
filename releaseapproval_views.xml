<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="warehousetpcodes_view_form" model="ir.ui.view">
        <field name="name">bsiedi.warehousetpcodes.view.form</field>
        <field name="model">bsiedi.warehousetpcodes</field>
        <field name="priority">10</field>
        <field name="active" eval="True"/>
        <field name="arch" type="xml">
            <form duplicate="True" edit="True">
                <sheet>
                    <group>
                        <group>
                          <field name="name"/>
                          <field name="trading_partner"/>
                          <field name="warehouse_id"/>
                          <field name="warehouse_code"/>
                        </group>
                    </group>
                </sheet>
              </form>
        </field>
    </record>
    <record id="view_warehousetpcodes_tree" model="ir.ui.view">
        <field name="name">bsiedi.warehousetpcodes.list</field>
        <field name="model">bsiedi.warehousetpcodes</field>
        <field name="priority">5</field>
        <field name="active" eval="True"/>
        <field name="arch" type="xml">
            <tree string="Warehouse Codes by Trading Partner" multi_edit="1" sample="1">
                <field name="name" readonly="1"/>
                <field name="trading_partner" string="Trading Partner"/>
                <field name="warehouse_id" string="Warehouse"/>
                <field name="warehouse_code" string="Warehouse Code"/>
            </tree>
        </field>
    </record>
    <record id="view_warehousetpcodes_search" model="ir.ui.view">
        <field name="name">bsiedi.warehousetpcodes.search</field>
        <field name="model">bsiedi.warehousetpcodes</field>
        <field name="priority">5</field>
        <field name="mode">primary</field>
        <field name="active" eval="True"/>
        <field name="arch" type="xml">
            <search string="Warehouse Codes">
                <field name="trading_partner"/>
                <separator/>
                <field name="warehouse_id"/>
            </search>
        </field>
    </record>
</odoo>
