<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="group_edi_user" model="res.groups">
        <field name="name">EDI User</field>
        <field name="comment">
            EDI User
        </field>
    </record>
    <record id="group_edi_admin" model="res.groups">
        <field name="name">EDI Administrator</field>
        <field name="comment">
            EDI Administrator
        </field>
        <field name="implied_ids" eval="[(4, ref('group_edi_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>
</odoo>