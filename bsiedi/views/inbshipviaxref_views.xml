<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="inbshipviaxref_view_form" model="ir.ui.view">
        <field name="name">bsiedi.inbshipviaxref.view.form</field>
        <field name="model">bsiedi.inbshipviaxref</field>
        <field name="priority">10</field>
        <field name="active" eval="True"/>
        <field name="arch" type="xml">
            <form duplicate="True" edit="True">
                <sheet>
                    <group>
                        <group>
                          <field name="name"/>
                          <field name="ship_descr"/>
                          <field name="ship_via_cd"/>
                          <field name="partner_id"/>
                          <field name="trading_partner_id"/>
                          <field name="scac"/>
                          <field name="trans_meth_code"/>
                          <field name="service_level_code"/>
                          <field name="ship_priority"/>
                          <field name="carrier_routing"/>
                          <field name="delivery_carrier"/>
                        </group>
                    </group>
                </sheet>
              </form>
        </field>
    </record>
    <record id="view_inbshipviaxref_tree" model="ir.ui.view">
        <field name="name">bsiedi.inbshipviaxref.list</field>
        <field name="model">bsiedi.inbshipviaxref</field>
        <field name="priority">5</field>
        <field name="active" eval="True"/>
        <field name="arch" type="xml">
            <tree string="Inbound Ship-Via Cross-References" multi_edit="1" sample="1">
                <field name="name" readonly="1"/>
                <field name="ship_descr" string="Shipping Description from EDI"/>
                <field name="ship_via_cd" string="Ship Via"/>
                <field name="partner_id" string="Customer/Vendor"/>
                <field name="trading_partner_id" string="Trading Partner"/>
            </tree>
        </field>
    </record>
</odoo>
